<%- include('partials/header') %>

<div class="jumbotron text-center">
  <div class="container">
    <i class="fas fa-key fa-6x"></i>
    <h1 class="display-3">To-day List</h1>
<p>Welcome <%=username.displayName %></p>
<% userWithTask.forEach((task)=>{ %>
    <p class="secret-text"><%= task.task %></p>
<% }) %>
    <hr>

    <a class="btn btn-light btn-lg" href="/logout" role="button">Log Out</a>
    <a class="btn btn-dark btn-lg" href="/submit" role="button">Submit a Task</a>
  </div>
</div>

<%- include('partials/footer') %>
